package application;

import javafx.event.ActionEvent;
import javafx.event.EventHandler;

public class HW1Controller implements EventHandler<ActionEvent> {
	HW1Model model = null;
	HW1View view = null;
	
	public HW1Controller(HW1Model m, HW1View v) {
		model = m;
		view = v;
		v.addEventHandler(this);
	}

	@Override
	public void handle(ActionEvent event) {
		System.out.println("Event happened: " + event.getSource());
	}

}
